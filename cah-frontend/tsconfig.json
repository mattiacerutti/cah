{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "module": "ESNext",
        "forceConsistentCasingInFileNames": true,
        "target": "ES6",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": false,
        "moduleResolution": "node",
        "verbatimModuleSyntax": false,
        "allowSyntheticDefaultImports": true
    }
}
