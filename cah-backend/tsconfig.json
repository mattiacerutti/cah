{
  "compilerOptions": {
    "target": "ES6",                                 
    "moduleResolution": "node10",                    
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,            
    "strict": false,                                    
    "skipLibCheck": true,
    // Path ./src to @
    "paths": {
      "@/*": ["./src/*"]
    },
    "baseUrl": "./",
  }
}
